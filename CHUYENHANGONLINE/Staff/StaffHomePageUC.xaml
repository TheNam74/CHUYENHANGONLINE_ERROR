<UserControl x:Class="CHUYENHANGONLINE.Staff.StaffHomePageUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CHUYENHANGONLINE.Staff"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Loaded="StaffHomePageUC_OnLoaded">
    <Canvas>
        <Label Name="HelloLabel" FontSize="20" Foreground="GhostWhite" Background="CadetBlue" Width="800">Xin chào</Label>
        <Button Name="SignOutButton" Canvas.Left="693" Background="DarkRed" Canvas.Top="1" Click="SignOutButton_OnClick">Đăng xuất</Button>

        <Button Name="ProviderListButton" Canvas.Left="328" Canvas.Top="152" Click="ProviderListButton_OnClick">Danh sách đối tác</Button>
        <Button Name="NewProviderListButton" Canvas.Left="332" Canvas.Top="225" Click="NewProviderListButton_OnClick">Duyệt hợp đồng</Button>
        <Button Name="ProfileButton" Canvas.Left="492" Canvas.Top="1" Click="ProfileButton_OnClick">Thông tin nhân viên</Button>
    </Canvas>
</UserControl>
